{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "context/auth/AuthContext.js",
    "pages/teams/[teamId]/members/[username].jsx",
    "components/buttons/SubmitTasksButton.jsx",
    "pages/teams/[teamId]/index.jsx",
    "components/users/AvatarPopup.jsx",
    "pages/teams/index.jsx",
    "components/groups/Group.jsx",
    "components/users/UserCard.jsx",
    "components/groups/CreateGroupForm.jsx",
    "components/tasks/CreateTasksForm.jsx",
    "pages/createteam.jsx",
    "pages/home.jsx",
    "pages/index.jsx",
    "context/AppContext.js",
    "context/combineContext.tsx",
    "context/TeamsContext.js",
    "hooks/inputs/useCheckbox.js",
    "components/users/AvatarRow.jsx",
    "components/layout/Modal.jsx",
    "components/teams/CreateTeamForm.jsx",
    "components/layout/Navbar/Navbar.tsx",
    "components/groups/Groups.jsx",
    "components/layout/Sidebar/Sidebar.jsx",
    "pages/register.jsx",
    "components/buttons/CollapseButton.tsx",
    "pages/_app.jsx",
    "pages/teams/[teamId]/groups/index.jsx",
    "pages/teams/[teamId]/groups/[groupId]/meetings/index.jsx",
    "pages/teams/[teamId]/groups/[groupId]/meetings/[meetingId]/index.jsx",
    "pages/teams/[teamId]/groups/[groupId]/index.jsx",
    "pages/teams/[teamId]/meetings/index.jsx"
, "components/teams/TeamsHeader.jsx"  ],
  "exclude": ["node_modules"]
}
